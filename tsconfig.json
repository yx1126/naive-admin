{
    "include": [
        "env.d.ts",
        "components.d.ts",
        "auto-imports.d.ts",
        "src/**/*.d.ts",
        "src/**/*.vue",
        "src/**/*.ts",
        "src/**/*.tsx"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "allowJs": true,
        "checkJs": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "esnext",
            "dom"
        ],
        "types": [
            "vite/client",
            "naive-ui/volar",
            "unplugin-vue-define-options/macros-global",
            "vite-plugin-svg-icons/client"
        ],
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "files": [],
    "references": [
        {
            "path": "./tsconfig.node.json"
        },
        {
            "path": "./tsconfig.app.json"
        }
    ],
    "vueCompilerOptions": {
        "target": 3,
        "plugins": [
            "@vue-macros/volar/define-options"
        ]
    }
}